{
  "name": "po-ai-local",
  "version": "1.0.0",
  "description": "PO AI Local Workspace - AI-powered product ownership assistant",
  "main": "local-app.js",
  "scripts": {
    "start": "node local-app.js",
    "dev": "node local-app.js",
    "logs": "node -e \"const fs=require('fs'),p=require('path'),os=require('os');const f=p.join(os.tmpdir(),'poai.log');if(fs.existsSync(f))console.log(fs.readFileSync(f,'utf-8').split('\\n').slice(-100).join('\\n'));else console.log('No log file found');\"",
    "init-db": "node scripts/init-db.js",
    "init-workspace": "node scripts/init-workspace.js",
    "seed-templates": "node scripts/seed-templates.js",
    "extract-metadata": "node scripts/extract-md-metadata.js"
  },
  "keywords": [
    "product-ownership",
    "ai",
    "llm",
    "mistral",
    "safe-agile"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "chromadb": "^3.2.2",
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.9",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "graphology": "^0.26.0",
    "graphology-metrics": "^2.4.0",
    "mammoth": "^1.11.0",
    "node-cron": "^4.2.1",
    "sqlite-vss": "^0.1.2",
    "sqlite3": "^5.1.7",
    "string-similarity": "^4.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
